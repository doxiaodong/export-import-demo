{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "strictNullChecks": false,
    // "noUnusedParameters": true,
    "noUnusedLocals": true,
    "baseUrl": "./src",
    "paths": {},
